{
  "name": "box-shadow",
  "author": "Guillermo Angulo <anyermo@gmail.com>",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0 --quiet",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "postinstall": "husky install",
    "supabase:types": "dotenv -e .env.local -- cross-var npx openapi-typescript %NEXT_PUBLIC_SUPABASE_URL%/rest/v1/?apikey=%NEXT_PUBLIC_SUPABASE_ANON_KEY% --output src/types/supabase.ts"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@styled-icons/boxicons-logos": "^10.23.0",
    "@styled-icons/boxicons-regular": "^10.23.0",
    "@styled-icons/boxicons-solid": "^10.23.0",
    "@styled-icons/material-outlined": "^10.28.0",
    "@styled-icons/typicons": "^10.18.0",
    "@supabase/supabase-js": "^1.7.7",
    "@supabase/ui": "^0.23.1",
    "hex-rgb": "^4.2.0",
    "next": "10.0.7",
    "next-pwa": "^5.0.5",
    "nextjs-progressbar": "^0.0.8",
    "polished": "^4.1.1",
    "prism-react-renderer": "^1.2.0",
    "react": "17.0.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "17.0.1",
    "react-hot-toast": "^1.0.2",
    "rgb-hex": "^3.0.0",
    "styled-components": "5.2.1",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-typescript": "^7.12.17",
    "@storybook/addon-essentials": "6.1.18",
    "@storybook/addons": "^6.1.21",
    "@storybook/react": "6.1.18",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.30",
    "@types/react": "^17.0.2",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.5.4",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "storybook-addon-next-router": "^2.0.4",
    "storybook-dark-mode": "^1.0.7",
    "typescript": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GuilleAngulo/box-shadow"
  },
  "bugs": {
    "url": "https://github.com/GuilleAngulo/box-shadow/issues"
  }
}
